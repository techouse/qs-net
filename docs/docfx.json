{
  "$schema": "https://raw.githubusercontent.com/dotnet/docfx/main/schemas/docfx.schema.json",
  "metadata": [
    {
      "src": [
        {
          "src": "../QsNet",
          "files": [
            "**/*.csproj"
          ],
          "exclude": [
            "**/QsNet.Tests/**"
          ]
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      },
      {
        "src": ".",
        "files": [
          "logo.png"
        ],
        "dest": "images"
      }
    ],
    "output": "_site",
    "template": [
      "default",
      "modern"
    ],
    "globalMetadata": {
      "_appLogoPath": "images/logo.png",
      "_appName": "QsNet",
      "_appTitle": "QsNet",
      "_appFaviconPath": "images/logo.png",
      "_enableSearch": true,
      "pdf": false
    }
  }
}